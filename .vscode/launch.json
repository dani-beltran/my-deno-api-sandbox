  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Deno sandbox debug",
      "request": "launch",
      "type": "pwa-node",
      "program": "app/main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "--unstable",
        "--allow-all",
        "--no-check"
      ],
      "attachSimplePort": 9229,
      "env": {
        "PORT": "8000",
        "ENV": "dev",
        "AUTH_HOST": "localhost:8080",
        "AUTH_CLIENT_ID": "deno-sandbox",
        "AUTH_CLIENT_SECRET": "66809fe3-3235-475f-aff5-e8aba041308b",
        "AUTH_REALM": "myrealm",
        "AUTH_SSL": "false",
      }
    }
  ]
}